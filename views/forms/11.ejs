            <!-- // END Header -->

           
           
            <!-- Header Layout Content -->
            <div class="mdk-header-layout__content">

                <div class="mdk-drawer-layout js-mdk-drawer-layout"
                     data-push
                     data-responsive-width="992px">
                    <div class="mdk-drawer-layout__content page">

                        <div class="container-fluid page__heading-container">
                            <div class="page__heading">
                                

                                <h1 class="m-0">Detailed Project Report(DPR)</h1>
                            </div>
                        </div>

                        <div class="container-fluid page__container">
           
                            <div class="card card-form">
                                <div class="card">
                                        
                                        <div class="card-header card-header-tabs-basic nav"
                                             role="tablist">
                                            <a href="/dpr/<%=data._id%>/1"
                                         
                                               
                                               role="tab"
                                               aria-controls="activity_all"
                                               aria-selected="true">About Cluster</a>
                                            <a href="/dpr/<%=data._id%>/2"
                                               
                                               role="tab"
                                               aria-selected="false">Info_Imp.Agency</a>
                                            <a href="/dpr/<%=data._id%>/3"
                                          
                                               
                                               role="tab"
                                               aria-selected="false">Info...SPV</a>
                                            <a href="/dpr/<%=data._id%>/4"
                                               
                                               role="tab"
                                               aria-selected="false">Info...Prod.Process</a>
                                            <a href="/dpr/<%=data._id%>/5"
                                               
                                               role="tab"
                                               aria-selected="false">Land Details</a>
                                            <a href="/dpr/<%=data._id%>/6"
                                               
                                               role="tab"
                                               aria-selected="false">Soft Intervention</a>
                                             
                                            <a href="/dpr/<%=data._id%>/7"
                                               
                                               role="tab"
                                               aria-selected="false">Hard Intervention</a>
                                            <a href="/dpr/<%=data._id%>/8"
                                               
                                               role="tab"
                                               aria-selected="false">Project Cost Details</a>
                                            <a href="/dpr/<%=data._id%>/9"
                                               
                                               role="tab"
                                               aria-selected="false">Info...Proj.Timeline</a>
                                            <a href="/dpr/<%=data._id%>/10"
                                               
                                               role="tab"
                                               aria-selected="false">ExpectedImpact</a>
                                            <a href="/dpr/<%=data._id%>/11"
                                               
                                               role="tab"
                                               class="active"
                                               aria-selected="false">TentativeBusiness Plan</a>
                                            <a href="/dpr/<%=data._id%>/12"
                                               
                                               role="tab"
                                               aria-selected="false">Info_Artisans</a>
                                        </div>
                                    </div>
                                <div class="row no-gutters">
                                    <div class="col-lg-4 card-body">
                                        
                                        
                                    </div>
                                    <div class="container-fluid page__container">
                                      <div class="card card-form">
                                          
                                          <div class="card-body">
                                             <p><strong class="headings-color">Annual Production</strong></p>
                                          </div>
                                          <div class="row no-gutters">
                                             <div class="card-form__body card-body">
                                             <form action="/dpr/<%=data._id%>/11?form=1" method="POST">
                                                
                                                  <div class="form-row">
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Name of Product</label>
                                                        <input required value="" type="text"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="apname"
                                                               required="true">
                                                       
                                                       
                                                    </div>
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Quantity </label>
                                                        <input required value="" type="number"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="apquantity"
                                                               required="true">
                                                        
                                                        
                                                    </div>
                                                    <div class="col mb-3">
                                                        
                                                         <label for="select01">Unit</label>
                                                         <select id="select01"
                                                             data-toggle="select"
                                                             name="apunit"
                                                              class="form-control">
                                                         <option selected="">One</option>
                                                         <option>Two </option>
                                                         <option>Three</option>
                                                         </select>
                                                     
                                                    </div>
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Rate(in Rs.)</label>
                                                        <input required value="" type="number" step="0.01"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="aprate"
                                                               required="true">
                                                       
                                                     
                                                    </div>
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Value(in Rs.)</label>
                                                        <input required value="" type="number" step="0.01"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="apvalue"
                                                               required="true">
                                                       
                                                     
                                                    </div>
                                                   
                                                  </div>
                                                  <button type="submit"
                                                    class="btn btn-primary">Add</button>
                                                  <div class="form-row">
                                                       <div class="col  mb-3"></div>
                                                  </div>
                                                  <div class="form-row">
                                                       <div class="col  mb-3">
                                                          <p><strong class="headings-color">Total Annual Product Turnover(in Rs.):</strong></p>
                                                      </div>
                                                  </div> 
                                              </form>
                                            </div>
                                          </div>
                                       </div>
                                   </div>
                                   
                                   <div class="container-fluid page__container">
                                      <div class="card card-form">
                                          <div class="card-body">
                                             <p><strong class="headings-color">Sales Turnover</strong></p>
                                          </div>
                                          <div class="row no-gutters">
                                             <div class="card-form__body card-body">
                                             <form  action="/dpr/<%=data._id%>/11?form=2" method="POST">
                                                
                                                  <div class="form-row">
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Name of Product</label>
                                                        <input required value="" type="text"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="stname"
                                                               required="true">
                                                       
                                                       
                                                    </div>
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Quantity </label>
                                                        <input required value="" type="number"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="stquantity"
                                                               required="true">
                                                        
                                                        
                                                    </div>
                                                    <div class="col mb-3">
                                                        
                                                         <label for="select01">Unit</label>
                                                         <select id="select01"
                                                             data-toggle="select"
                                                              class="form-control"
                                                              name="stunit">
                                                         <option selected="">One</option>
                                                         <option>Two </option>
                                                         <option>Three</option>
                                                         </select>
                                                     
                                                    </div>
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Rate(in Rs.)</label>
                                                        <input required value="" type="number" step="0.01"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="strate"
                                                               required="true">
                                                       
                                                     
                                                    </div>
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Value(in Rs.)</label>
                                                        <input required value="" type="number" step="0.01"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="stvalue"
                                                               required="true">
                                                       
                                                     
                                                    </div>
                                                   
                                                  </div>
                                                  <button type="submit"
                                                    class="btn btn-primary">Add</button>
                                                   <div class="form-row">
                                                       <div class="col  mb-3"></div>
                                                  </div>
                                                  <div class="form-row">
                                                       <div class="col  mb-3">
                                                          <p><strong class="headings-color">Total Annual Sales Turnover(in Rs.):</strong></p>
                                                      </div>
                                                  </div>  
                                              </form>
                                            </div>
                                          </div>
                                       </div>
                                   </div>
                                    <div class="container-fluid page__container">
                                      <div class="card card-form">
                                          <div class="card-body">
                                             <p><strong class="headings-color">Market Linkage</strong></p>
                                          </div>
                                          <div class="row no-gutters">
                                             <div class="card-form__body card-body">
                                             <form  action="/dpr/<%=data._id%>/11?form=3" method="POST">
                                                
                                                  <div class="form-row">
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Any Buy Back Agreement*</label>
                                                        <input required value="" type="text"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="buyback"
                                                               required="true">
                                                       
                                                       
                                                    </div>
                                                  </div>
                                                  <div class="form-row">
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Any Tie Up with Dedicated Sales Outlet/Showroom*</label>
                                                        <input required value="" type="text"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="tieup"
                                                               required="true">
                                                       
                                                       
                                                    </div>
                                                  </div>
                                                  <div class="form-row">
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Any Others Mode of Banking*</label>
                                                        <input required value="" type="text"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="bankingmode"
                                                               required="true">
                                                       
                                                       
                                                    </div>
                                                  </div>
                                                  <div class="form-row">
                                                    <div class="col mb-3">
                                                        <label for="validationSample01">Any Others Information/Remark*</label>
                                                        <input required value="" type="text"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="otherinfo"
                                                               required="true">
                                                       
                                                       
                                                    </div>
                                                  </div>
                                                  <button type="submit"
                                                    class="btn btn-primary">Save</button>
                                                   <button type="reset"
                                                    class="btn btn-primary">Reset</button> 
                               
                                                  
                                              </form>
                                            </div>
                                          </div>
                                       </div>
                                   </div>
                                   
                               
                               
                                </div>
                          </div>       
                        
                        </div>
                    </div>


                    



                    <!-- // END drawer-layout__content -->

                    <%- include('partials/sidebar') %>
                </div>
                <!-- // END drawer-layout -->

            </div>
