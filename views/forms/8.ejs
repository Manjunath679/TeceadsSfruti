                                                        <label for="validationSample01">IA Share/Contribution(in lakh)</label>
                                                        <input required value=""  type="number" step="0.01"
                                                               name="hiia"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               pattern="[0-9]{.}"
                                                               required="true">
                                                       
                                                     
                                                    </div>
                                                   
                                                  </div>
