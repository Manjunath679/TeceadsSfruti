                                                        <input required value="" type="number" step="0.01"
                                                               class="form-control"
                                                               id="validationSample01"
                                                               placeholder=" "
                                                               name="hetotal"
                                                               required="true">
                                                        
                                                        
                                                    </div>
                                                   </div>
                                                  
